  "version": "0.1.0",
  "description": "Example for Listick based on react",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build:vendors": "node_modules/.bin/webpack --config webpack.config.vendor.js",
    "build:client": "node_modules/.bin/webpack",
