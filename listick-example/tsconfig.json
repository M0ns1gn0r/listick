{
	"compilerOptions": {
	  "baseUrl": ".",
	  "module": "es2015",
	  "moduleResolution": "node",
	  "target": "es5",
	  "jsx": "react",
	  "experimentalDecorators": true,
	  "emitDecoratorMetadata": true,
	  "sourceMap": true,
	  "skipDefaultLibCheck": true,
	  "strict": true,
	  "lib": ["es6", "dom"]
	},
	"exclude": [
		"bin",
		"node_modules"
	]
  }
  